{
    "explorer.confirmDelete": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Andromeda",
    "prettier.tabWidth": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        //"**/node_modules": true,
        "**/.DS_Store": true
    },
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**",
      ],
      "liveSassCompile.settings.formats": [
          {
              "format": "expanded",
              "extensionName": ".css",
              "savePath": "/docs"
          }
    ],
    "cSpell.ignoreWords": [
        "Mottram"
    ]
}